		<rich_text>
</rich_text>
		<node custom_icon_id="0" foreground="" is_bold="False" name="Art And War" prog_lang="custom-colors" readonly="False" tags="" ts_creation="0.0" ts_lastsave="1630118656.01" unique_id="921">
