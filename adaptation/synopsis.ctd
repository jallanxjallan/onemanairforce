					<node custom_icon_id="0" foreground="" is_bold="False" name="Birth Of RI-002" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1629617323.63" ts_lastsave="1630670743.49" unique_id="1052">
						<rich_text link="file c2NlbmVzL2JpcnRoX29mX3JpLTAwMi5tZA==">Document</rich_text>
						<rich_text>
</rich_text>
						<rich_text link="node 367">Fictitious Registration</rich_text>
