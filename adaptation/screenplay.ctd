		<node custom_icon_id="0" foreground="" is_bold="False" name="Julia finds Emeria Bio" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1630809337.5" ts_lastsave="1637231012.8" unique_id="1113">
			<rich_text>
