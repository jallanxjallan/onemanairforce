		<node custom_icon_id="0" foreground="" is_bold="False" name="Muharto Visits Post" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1629601100.11" ts_lastsave="1635077867.39" unique_id="1043">
			<rich_text link="node 918">Cameron Offers To Help</rich_text>
			<rich_text> 
</rich_text>
			<rich_text link="file c2VxdWVuY2VzL211aGFydG9fdmlzaXRzX3Bvc3QubWQ=">Document</rich_text>
