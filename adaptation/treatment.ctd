			</node>
			<node custom_icon_id="0" foreground="" is_bold="False" name="Syds Triple Shift" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1607933775.38" ts_lastsave="1613203035.14" unique_id="774">
