					<rich_text> 
[[Bob Walters Bitches]]</rich_text>
				</node>
				<node custom_icon_id="0" foreground="" is_bold="False" name="Primitivo Introduces Muharto" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1605701801.84" ts_lastsave="1611572613.09" unique_id="604">
					<rich_text link="file c2NlbmVzL211aGFydG9fbWVldHNfcHJpbWl0aXZvLm1k">Scene</rich_text>
				</node>
				<node custom_icon_id="0" foreground="" is_bold="False" name="Ining Buys Drinks" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1605701801.81" ts_lastsave="1611573305.94" unique_id="545">
