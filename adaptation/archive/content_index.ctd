		<node custom_icon_id="0" foreground="" is_bold="False" name="Boedi Tells All" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1621156740.66" ts_lastsave="1629445688.77" unique_id="1004">
			<rich_text link="node 990">Risky Missions</rich_text>
			<rich_text> 
</rich_text>
			<rich_text link="node 162">Not Ready For Airplanes</rich_text>
