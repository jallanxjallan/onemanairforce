			<node custom_icon_id="0" foreground="" is_bold="False" name="First Muharto Interview" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1579859816.6" ts_lastsave="1580722278.03" unique_id="155">
				<rich_text></rich_text>
				<node custom_icon_id="0" foreground="" is_bold="False" name="Introducing Bob" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1580722252.74" ts_lastsave="1580723349.74" unique_id="57">
					<rich_text>• date: 28 September 1988
• location: Muharto living room
• </rich_text>
					<rich_text link="file c2NlbmVzL2ludHJvZHVjaW5nX2ZyZWViZXJnLm1k">Treatment</rich_text>
					<rich_text>

</rich_text>
					<rich_text link="node 8 first_muharto_interview">Cameron meets Petit Muharto</rich_text>
					<rich_text> 
</rich_text>
					<node custom_icon_id="0" foreground="" is_bold="False" name="Surprise Call" prog_lang="custom-colors" readonly="False" tags="" ts_creation="1579835201.29" ts_lastsave="1580722263.26" unique_id="34">
						<rich_text>• 1 June 1947
• Muhartos office Maguwo
